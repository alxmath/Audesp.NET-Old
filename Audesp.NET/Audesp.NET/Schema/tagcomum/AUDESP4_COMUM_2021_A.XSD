   <!-- ======================================================================== -->

   <!-- Valida numeros  -->

   <!-- define um número Codigo Licitacao  -->
   <xs:simpleType name="CodigoLicitacao_t">
      <xs:restriction base="xs:long">
         <xs:minInclusive value="2021000000000"/>
         <xs:maxInclusive value="2021999999999"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="CodigoAjuste_t">
      <xs:restriction base="xs:long">
         <xs:minInclusive value="2021000000000"/>
         <xs:maxInclusive value="2021999999999"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="CodigoTermo_t">
      <xs:restriction base="xs:long">
         <xs:minInclusive value="2021000000000"/>
         <xs:maxInclusive value="2021999999999"/>
      </xs:restriction>
   </xs:simpleType>
   
   
   <!-- define um número Codigo Licitacao  -->
   <xs:simpleType name="Codigo_t">
      <xs:restriction base="xs:long">
         <xs:minInclusive value="0000000000000"/>
         <xs:maxInclusive value="9999999999999"/>
      </xs:restriction>
   </xs:simpleType>
   

   <!-- define um número Inteiro maior que 0 até o limite de 32767 -->
   <xs:simpleType name="InteiroRestritoMaiorZero_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="32767"/>
      </xs:restriction>
   </xs:simpleType>
   
   
   <!-- define um número Inteiro maior que 0  -->
   <xs:simpleType name="InteiroMaiorZero_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="1"/>
         <xs:maxInclusive value="99999"/>
      </xs:restriction>
   </xs:simpleType>
   
      <!-- define um número Inteiro maior que 0  -->
   <xs:simpleType name="InteiroMaiorIgualZero_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="99999"/>
      </xs:restriction>
   </xs:simpleType>
   
     <!-- define um número Inteiro Positivo -->
   <xs:simpleType name="InteiroPositivo_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="1"/>
         <xs:maxInclusive value="999999"/>
      </xs:restriction>
   </xs:simpleType> 
   
   <!-- define um número Inteiro maior que 0  -->
   <xs:simpleType name="InteiroMaior_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="999999999"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="InteiroDe1Ate999_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="1"/>
         <xs:maxInclusive value="999"/>
      </xs:restriction>
   </xs:simpleType>

   <!-- define um número decimal maior que 0 -->
   <xs:simpleType name="DecimalMaiorQueZero_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="2"/>
         <xs:minInclusive value="0.01"/>
         <xs:maxInclusive value="9999999999999.00"/>
      </xs:restriction>
   </xs:simpleType>

   <!-- define um número decimal maior que 0  -->
  <xs:simpleType name="Decimal20-5_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="5"/>
         <xs:minInclusive value="0.00001"/>
         <xs:maxInclusive value="999999999999999.99999"/>
      </xs:restriction>
   </xs:simpleType>

    <!-- define um número decimal -->
   <xs:simpleType name="Decimal17-2_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="2"/>
         <xs:minInclusive value="0.00"/>
         <xs:maxInclusive value="999999999999999.99"/>
      </xs:restriction>
   </xs:simpleType>

    <!-- define um número decimal maior que 0-->
   <xs:simpleType name="DecimalMaiorQueZero17-2_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="2"/>
         <xs:minInclusive value="0.01"/>
         <xs:maxInclusive value="999999999999999.99"/>
      </xs:restriction>
   </xs:simpleType>

   <!-- define um número decimal maior que 0  -->
   <xs:simpleType name="Decimal15-5_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="5"/>
         <xs:minInclusive value="0.01"/>
         <xs:maxInclusive value="9999999999.99999"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="LatitudeLongitude_t">
      <xs:restriction base="xs:decimal">
         <xs:pattern value="-?\d{2}\.\d{7}"/>
         <xs:totalDigits value="9"/>
         <xs:fractionDigits value="7"/>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="PercentualPositivo_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="2"/>
         <xs:minInclusive value="0.00"/>
         <xs:maxInclusive value="100.00"/>
      </xs:restriction>
   </xs:simpleType>
   
   <xs:simpleType name="PercentualPositivoMaiorQueZero_t">
      <xs:restriction base="xs:decimal">
         <xs:fractionDigits value="2"/>
         <xs:minInclusive value="0.01"/>
         <xs:maxInclusive value="100.00"/>
      </xs:restriction>
   </xs:simpleType>

   <!-- ======================================================================== -->
   <!-- VALIDAÇÃO DE DATA -->
   <xs:simpleType name="ValidaData_t">
      <xs:restriction base="xs:date">
         <xs:pattern value="\d{4}-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])"/>
      </xs:restriction>
   </xs:simpleType>

   <!-- define um Ano  -->
   <xs:simpleType name="Ano4_t">
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="1900"/>
         <xs:maxInclusive value="2050"/>
      </xs:restriction>
   </xs:simpleType>


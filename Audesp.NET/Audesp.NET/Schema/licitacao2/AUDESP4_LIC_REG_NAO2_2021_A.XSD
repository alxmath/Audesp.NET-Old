   <!-- ========================================================================= -->
   <xs:annotation>
      <xs:documentation>
         <DataCriacao>2020-12-15</DataCriacao>
         <DataModificacao>2020-12-15</DataModificacao>
         <VersaoRevisaoAnterior>2021_A</VersaoRevisaoAnterior>
         <VersaoRevisaoNova>2021_A</VersaoRevisaoNova>
         <Descricao>Licitação - Concorrencia Internacional</Descricao>
         <Justificativa>Licitação - Concorrencia Internacional</Justificativa>
         <Autor>DSIS</Autor>
      </xs:documentation>
   </xs:annotation>
   <!-- ========================================================================= -->
   <!-- 
      1. Ata de Registro de Preço:Sim - 
      2 Compras e Servicos de TI            
   
             Compras e Serviços	Compras e Serviços de TI	Obras e Serviços de Engenharia
   
   -->

   <!-- ========================================================================= -->
   <!-- root -->
   <xs:element name="Licitacao" >

   <!-- ========================================================================= -->

   <xs:complexType>

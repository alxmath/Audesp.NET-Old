		  <xs:choice>
                <xs:element name="AudienciaPublicaNao" type="tag:Nao_t" minOccurs="1" maxOccurs="1"/>
                <xs:element name="AudienciaPublica-Dt" type="tag:ValidaData_t" minOccurs="1" maxOccurs="1"/>
            </xs:choice>
        

         <xs:element name="Descritor" type="gen:Descritor_t" minOccurs="1" maxOccurs="1"/>
         <xs:element name="RegistroPrecosNao2" type="RegistroPrecosNao2_t" minOccurs="1"
            maxOccurs="50"/>
      </xs:sequence>
   </xs:complexType>
   </xs:element>

   <!-- ========================================================================= -->
   <!-- ========================================================================= -->
   <!-- Licitacao Tipo 2 = Concorrencia  Internacional -->
